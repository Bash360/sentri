{
  "name": "sentri-api",
  "version": "1.0.0",
  "description": "Sentri API Service",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migrate:generate": "npm run typeorm -- -d src/config/dataSource.ts migration:generate src/migration/migrate",
    "migrate:run": "npm run typeorm -- -d src/config/dataSource.ts migration:run"
  },
  "keywords": [],
  "author": "Mark Bashir",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
